{
  "name": "payroll-contracts",
  "scripts": {
    "eslint": "eslint \"**/*.{js,ts}\"",
    "solhint": "solhint contracts/**/*.sol",
    "lint": "yarn eslint && yarn solhint",
    "lint:fix": "yarn eslint --fix && yarn solhint --fix",
    "prettier": "prettier \"**/*.{ts,js,sol}\"",
    "prettier:check": "yarn prettier --check",
    "prettier:fix": "yarn prettier --write",
    "compile": "hardhat compile",
    "test": "yarn clean && yarn compile && hardhat test",
    "typechain": "TS_NODE_TRANSPILE_ONLY=true hardhat typechain",
    "coverage": "hardhat coverage",
    "flatten": "hardhat flatten",
    "deploy": "hardhat deploy --network",
    "docgen": "hardhat docgen",
    "reinstall": "rm -rf node_modules/ && rm -f yarn.lock && yarn",
    "clean": "hardhat clean",
    "size": "hardhat size-contracts",
    "verify": "hardhat etherscan-verify --license AGPL-3.0 --force-license --solc-input --network",
    "start-fork": "FORK_ENABLED=true hardhat node --no-reset --no-deploy",
    "decodeTx": "hardhat run scripts/decodeTxParams.ts",
    "replayTx": "hardhat run scripts/replayTx.ts --network bsc",
    "callStatic": "hardhat run scripts/contractCallStatic.ts --network bsc",
    "verifyAllowance": "hardhat run scripts/verifyAllowance.ts --network"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.12",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.7.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.2",
    "@typechain/ethers-v5": "^8.0.5",
    "@typechain/hardhat": "^3.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.7.1",
    "hardhat-abi-exporter": "^2.8.0",
    "hardhat-contract-sizer": "^2.2.0",
    "hardhat-deploy": "^0.9.14",
    "hardhat-docgen": "^1.1.2",
    "hardhat-gas-reporter": "^1.0.6",
    "prettier": "^2.6.0",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.4.0",
    "typechain": "^6.1.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "=16"
  },
  "dependencies": {
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "@uniswap/v3-core": "^1.0.1",
    "@uniswap/v3-periphery": "^1.4.0",
    "@wink-financial/wink-assets": "^1.0.9",
    "solhint-plugin-prettier": "^0.0.5"
  }
}
